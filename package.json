  "server:dev": "cd api && npm run dev",
  "client": "cd client && npm start",
  "client:dev": "cd client && npm run dev"
},
 "devDependencies": {
  "concurrently": "^9.1.0"
 },
 "dependencies": {
  "vite": "^6.0.5"
 }
}
